plugin.tx_rkworder  {
    persistence {
        // cat=plugin.tx_rkworder_rkworder//a; type=string; label=Default storage PID
        storagePid = 1
    }

    settings {

        # cat=plugin.tx_rkworder/a; type=boolean; label=Disable admin mails?
        disableAdminMails = 0

		# cat=plugin.tx_rkworder//a; type=integer; label=Username of BE-User if none is set in plugin
		fallbackBackendUserForAdminMails = fallback
    }
}

config {
    doctype = html5
    xmlprologue = none

    inlineStyle2TempFile = 0
    removeDefaultJS = 0
    compressJs = 0
    concatenateJs = 0
    compressCss = 0
    concatenateCss = 0

    no_cache = 1
    debug = 0
    xhtml_cleaning = 0
    admPanel = 0
    disableAllHeaderCode = 1
    sendCacheHeaders = 0
    absRefPrefix = /
    contentObjectExceptionHandler = 0
}

page = PAGE
page {
    10 = TEXT
    10.value = Hallo Welt!
}